{
  "name": "dtable-sdk",
  "version": "0.0.2",
  "description": "dtable sdk",
  "main": "./dist/dtable.js",
  "scripts": {
    "clean": "rm -rf dist && mkdir dist",
    "example": "export DEBUG=dtable* && babel-node ./example/example.js",
    "build:pkg-js": "./node_modules/.bin/babel src --out-dir dist",
    "build:pkg-bin": "./node_modules/.bin/babel bin --out-dir dist/bin --copy-files",
    "build": "npm run clean && npm run build:pkg-js"
  },
  "files": [
    "dist",
    "package.json",
    "package-lock.json"
  ],
  "dependencies": {
    "dtable-store": "0.0.12",
    "axios": "^0.19.0",
    "form-data": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/node": "^7.5.0",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-transform-modules-commonjs": "^7.5.0",
    "@babel/preset-env": "^7.5.0",
    "@babel/register": "^7.4.4",
    "babel-register": "^6.26.0",
    "debug": "^4.1.1"
  }
}
