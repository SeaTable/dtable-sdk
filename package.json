{
  "name": "dtable-sdk",
  "version": "0.0.15",
  "description": "dtable sdk",
  "main": "./dist/dtable.js",
  "scripts": {
    "example": "export DEBUG=dtable* && babel-node ./example/example.js",
    "test": "export DEBUG=dtable* && babel-node ./test/complex-test.js",
    "build:pkg-lib": "rm -rf lib && mkdir lib && ./node_modules/.bin/babel src --out-dir lib",
    "build:pkg-dist": "rm -rf dist && mkdir dist && export BABEL_ENV=production && webpack",
    "prepublishOnly": "npm run build:pkg-lib && npm run build:pkg-dist"
  },
  "files": [
    "dist",
    "package.json",
    "package-lock.json"
  ],
  "dependencies": {
    "axios": "^0.19.0",
    "dtable-store": "0.0.56",
    "form-data": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/node": "^7.5.0",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-transform-modules-commonjs": "^7.5.0",
    "@babel/preset-env": "^7.5.0",
    "@babel/register": "^7.4.4",
    "babel-register": "^6.26.0",
    "debug": "^4.1.1",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
